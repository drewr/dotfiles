# -*- sh -*-

alias gl="git log -5 --pretty=oneline"
alias gb="git branch"
alias gs="git status"
alias gco="git checkout"
alias uberclean="mvn clean && echo uberclean done"
alias uberbuild="uberclean && mvn -Dnotest && echo uberbuild done"
alias ubertest="uberbuild && mvn test && echo ubertest done"
alias assemble="mvn assembly:assembly && echo assembly done"
alias pj="python -m json.tool"
alias e256="TERM=xterm-256color emacs"
alias kssh="knife sq ssh"
alias kst="knife sq status"
alias kex="knife sq exec"
