#!/bin/sh

PRE=${PRE:-drewr}

if [ "x" == "x$1" ]; then
  echo need key name
  exit 99
fi

tmp=$(mktemp -t s3cat)
cat >$tmp
s3c es.${PRE} put -P \
   --mime-type="text/plain; charset=utf-8" \
   --encoding=utf-8 \
   $tmp s3://users.elasticsearch.org/${PRE}/$1
rm $tmp
echo http://users.elasticsearch.org/${PRE}/$1
