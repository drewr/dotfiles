#!/bin/sh

KEY=${KEY:-drewr}

if [ "x" == "x$1" ]; then
  echo need key name
  exit 99
fi

tmp=$(mktemp -t s3cat)
cat >$tmp
s3c es.${KEY} put -P \
   --mime-type="text/plain; charset=utf-8" \
   --encoding=utf-8 \
   $tmp s3://users.elasticsearch.org/$1
rm $tmp
echo http://users.elasticsearch.org/$1
